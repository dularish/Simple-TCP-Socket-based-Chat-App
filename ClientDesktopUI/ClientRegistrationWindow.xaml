<Window x:Class="ClientDesktopUI.ClientRegistrationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClientDesktopUI"
        mc:Ignorable="d"
        Title="ClientRegistrationWindow" Height="450" Width="800" >
    <Grid>
        <StackPanel>
            <TextBlock>Please enter your unique login Id that you would like to be identified with</TextBlock>
            <TextBox Name="_loginIdBox" />
            <Button Name="_proceedBtn" Click="_proceedBtn_Click" Content="Proceed" />
        </StackPanel>
    </Grid>
</Window>
